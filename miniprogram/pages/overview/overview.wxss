page{
  background-color: #fdf7ae;
}

.container {
  padding: 20rpx;
}

.table-container {
  border: 1px solid #ccc;
  margin-bottom: 20rpx;
}

.table-header {
  display: flex;
  background-color: #f4f4f4;
}

.table-cell {
  width: 200rpx;
  flex: 1;
  text-align: center;
  padding: 10rpx;
  border-right: 1px solid #ccc;
}

.table-cell:last-child {
  border-right: none;
}

.table-body {
  display: flex;
  flex-wrap: wrap;
}

.chart {
  width: 100%;
  height: 400rpx;
  margin-top: 20rpx;
}

.button-container {
  display: flex;
  justify-content: space-around;
  margin-bottom: 10rpx;
}

.download{
  position: fixed; /* 固定定位 */
  bottom: 20px; /* 距离页面底部 20px */
  right: 20px; /* 距离页面右侧 20px */
  width: 100rpx; /* 按钮宽度 */
  height: 100rpx; /* 按钮高度 */
  border-radius: 50%;
  border: none; /* 去掉默认边框 */
  z-index: 999; /* 确保按钮在最上层 */
  background-color: #67C9C1;
  opacity: 0.8;
}
.download image{
  width: 80rpx;
  height: 80rpx;
  padding: 10rpx;
}
